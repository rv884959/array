#include<stdio.h>
int main()
{
    int arr[]={1,2,3,4,5};
    int length=sizeof(arr)/sizeof(arr[0]);
    printf("original array");
    for(int i=0;i<length;i++)
    {
        printf("%D",arr[i]);
    }
    printf("\n");
    printf("array in reverse order");
    for(int i=length-1;i>=0;i--)
    {
        printf("%d",arr[i]);
    }
    return 0;
}
